<h1>New House</h1>

<%= form_for @house do |f| %>
  <div class="form_meta_field">
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </div>
  <div class="form_meta_field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form_meta_field">
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>

  <%= f.submit %>
<% end %>


<%= link_to 'Back', houses_path(@house) %>
