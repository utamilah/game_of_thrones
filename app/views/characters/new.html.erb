<h1>New Character</h1>

<%= form_for [@house, @character] do |f| %>
  <div class="form_meta_field">
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </div>
  <div class="form_meta_field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <%= f.submit %>
<% end %>


<%= link_to 'Back', house_path(@house) %>
